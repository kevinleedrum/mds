{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "component-library": {
      "projectType": "library",
      "root": "component-library",
      "sourceRoot": "component-library/lib",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "component-library/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "component-library/tsconfig.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
