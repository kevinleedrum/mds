{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": ["src", "config"],
  "exclude": [
    "node_modules",
    "dist",
    "docs",
    "hydrate",
    "loader",
    "www"
  ]
}
