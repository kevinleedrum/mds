{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.0.0"
  },
  "scripts": {
    "build": "yarn mds:build && yarn mds-react:build",
    "mds:build": "yarn workspace @moxiworks/mds build",
    "mds:build-mds": "yarn workspace @moxiworks/mds build:mds",
    "mds:build-react": "yarn workspace @moxiworks/mds build:react",
    "mds:build-docs": "yarn workspace @moxiworks/mds build:docs",
    "mds:release-docs": "yarn workspace @moxiworks/mds docs:deploy",
    "mds-react:build": "yarn workspace @moxiworks/mds-react build"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.6.1"
}
